{
    "editor.minimap.enabled": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.editor.enablePreview": false,
    "security.workspace.trust.untrustedFiles": "open",
    "elixirLS.fetchDeps": false,
    "workbench.startupEditor": "none",
    "security.workspace.trust.enabled": false,
    "go.autocompleteUnimportedPackages": true,
    "git.ignoreLegacyWarning": true,
    "liveSassCompile.settings.watchOnLaunch": true,
    "editor.inlineSuggest.enabled": true,
    "workbench.editor.empty.hint": "hidden",
    "github.copilot.editor.enableAutoCompletions": true,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "markdown",
        "scminput"
    ],
    "backgroundCover.imagePath": "/Users/horiuchikei/Pictures/asukasaito23.jpg",
    "files.autoSave": "afterDelay",
    "[php]": {
        "editor.defaultFormatter": "obliviousharmony.vscode-php-codesniffer"
    },
    "editor.wordSeparators": "`~!@#%^&*()-=+[{]}\\|;:'\",.<>/?",
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    },
    "editor.formatOnPaste": true,
    "bladeFormatter.format.wrapAttributes": "preserve",
    "workbench.colorTheme": "Darcula",
    "backgroundCover.opacity": 0.1,
    "backgroundCover.blur": 2,
    "backgroundCover.sizeModel": "cover",
    "python.analysis.autoImportCompletions": true,
    "phpunit.debuggerConfig": "",
    "phpunit.command": "herd php artisan test",
    "phpunit.php": "",
    "phpunit.args": [

        "--configuration ${workspaceFolder}/phpunit.xml",
    ],
    "php-cs-fixer.formatHtml": true,
    "phpCodeSniffer.autoExecutable": true,
    "artisan.docker.command": "",
    "artisan.php.location": "herd php",
    "editor.fontSize": 13,
    "namespaceResolver.sortAlphabetically": true,
    "namespaceResolver.sortOnSave": true,
    "sync.sourceRepository": "hrorry/vscode-settings",
}